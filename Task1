CREATE ROLE "Giuseppe" WITH
LOGIN
NOSUPERUSER
NOCREATEDB
NOCREATEROLE
INHERIT
NOREPLICATION
CONNECTION LIMIT -1
PASSWORD 'Pizza';


CREATE DATABASE cars
WITH
OWNER = "Giuseppe"
ENCODING = "UTF8"
LC_COLLATE = 'Polish_Poland.1250'
LC_Ctype = 'Polish_Poland.1250'
TABLESPACE = pg_default
CONNECTION LIMIT = -1
